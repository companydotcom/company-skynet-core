{
  "extends": [
    "airbnb-base",
    "plugin:jest/recommended"
  ],
  "rules": {
    "no-console": 0,
    "import/prefer-default-export": 0,
    "arrow-parens": [
      2,
      "as-needed"
    ],
    "max-len": [
      2,
      {
        "ignoreComments": true,
        "ignoreTemplateLiterals": true,
        "ignoreStrings": true
      }
    ]
  }
}